@model UrlInfos
@{
    ViewBag.Title = "遗失歌曲对比";
    Layout = "_Layout";
}

<form class="validation-form" method="post" asp-controller="CrossValidation" asp-action="crossValidation" asp-antiforgery="true">
    <h2 class="export-title">
        遗失歌曲对比
    </h2>
    <div class="form-group">
        <div class="input-group input-group-lg">
            <span class="input-group-addon">http://www.xiami.com/space/lib-song/u/</span>
            <input type="hidden" asp-for="@Model.XiamiUrlInfo.UrlPrefix" value="http://www.xiami.com/space/lib-song/u/" />
            <input type="text" class="form-control" placeholder="请按照教程输入内容" asp-for="@Model.XiamiUrlInfo.UrlStr" />
        </div>
    </div>
    <div class="form-group">
        <div class="input-group input-group-lg">
            <span class="input-group-addon">http://music.163.com/playlist?id=</span>
            <input type="hidden" asp-for="@Model.CloudUrlInfo.UrlPrefix" value="http://music.163.com/playlist?id=" />
            <input type="text" class="form-control" placeholder="请按照教程输入内容" asp-for="@Model.CloudUrlInfo.UrlStr" />
        </div>
    </div>
    <button type="submit" class="btn btn-primary btn-lg export-btn">对比</button>
</form>
